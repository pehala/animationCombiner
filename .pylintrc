[FORMAT]
max-line-length=120

disable=duplicate-code,  # reports false alarms AND can't be disabled locally; pylint issue #214
        fixme,  # ignore TODOs
        redefined-outer-name,
        too-many-arguments,
        too-few-public-methods,
        no-member

[BASIC]
good-names=i,j,k,
           animationCombiner
           logger,